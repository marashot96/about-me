    "from math import floor\n",
    "\n",
    "def factor_count(n, p):\n",
    "    count = 0\n",
    "    power = p\n",
    "    while power <= n:\n",
    "        count += n // power\n",
    "        power *= p\n",
    "    return count\n",
    "\n",
    "def prime_factors(b):\n",
    "    i = 2\n",
    "    factors = {}\n",
    "    while i * i <= b:\n",
    "        while b % i == 0:\n",
    "            factors[i] = factors.get(i, 0) + 1\n",
    "            b //= i\n",
    "        i += 1\n",
    "    if b > 1:\n",
    "        factors[b] = 1\n",
    "    return factors\n",
    "\n",
    "def trailing_zeros(n: int, base: int) -> int:\n",
    "    factors = prime_factors(base)\n",
    "    return min(factor_count(n, p) // exp for p, exp in factors.items())"
   ]
